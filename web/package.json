    "@sveltejs/kit": "^2.53.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^22",
    "@types/set-cookie-parser": "^2.4.10",
    "@vitejs/plugin-basic-ssl": "^2.1.4",
    "@vitest/browser-playwright": "^4.0.18",
    "cookie": "^0.7.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
