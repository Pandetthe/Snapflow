{
  "$schema": "http://json.schemastore.org/template",
  "author": "Pandetthe",
  "classifications": [
    "Snapflow",
    "AspNet",
    "Endpoint",
    "Query"
  ],
  "name": "Snapflow Endpoint for Query",
  "identity": "Snapflow.AspNet.QueryEndpointTemplate",
  "shortName": "snapflow-aspnet-queryendpoint",
  "description": "Complete endpoint using query in Snapflow application.",
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "primaryOutputs": [
    {
      "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
      "path": "Example1.cs"
    }
  ],
  "postActions": [
    {
      "id": "openInEditor",
      "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
      "description": "Opens Example1.cs in the editor.",
      "manualInstructions": [],
      "actionId": "84C0DA21-51C8-4541-9940-6CA19AF04EB6",
      "args": {
        "files": "0"
      },
      "continueOnError": true
    }
  ],
  "sourceName": "Example1",
  "defaultName": "Example1",
  "symbols": {
    "namespace": {
      "description": "namespace for the generated code",
      "replaces": "SnapflowAspNet",
      "type": "parameter"
    }
  }
}