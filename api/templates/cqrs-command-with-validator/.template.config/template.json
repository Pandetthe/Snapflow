{
  "$schema": "http://json.schemastore.org/template",
  "author": "Pandetthe",
  "classifications": [
    "Snapflow",
    "CQRS",
    "Command",
    "Validator"
  ],
  "name": "Snapflow CQRS Command (with Validator)",
  "identity": "Snapflow.CQRS.CommandTemplate.WithValidator",
  "shortName": "snapflow-cqrs-command-validator",
  "description": "Complete set of classes to implement a CQRS Command (with Validator) in Snapflow application.",
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "primaryOutputs": [
    {
      "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
      "path": "Example1Command.cs"
    },
    {
      "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
      "path": "Example1CommandHandler.cs"
    },
    {
      "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
      "path": "Example1CommandValidator.cs"
    }
  ],
  "postActions": [
    {
      "id": "openInEditor",
      "condition": "(HostIdentifier != \"dotnetcli\" && HostIdentifier != \"dotnetcli-preview\")",
      "description": "Opens Example1Command.cs in the editor.",
      "manualInstructions": [],
      "actionId": "84C0DA21-51C8-4541-9940-6CA19AF04EB6",
      "args": {
        "files": "0"
      },
      "continueOnError": true
    }
  ],
  "sourceName": "Example1",
  "defaultName": "Example1",
  "symbols": {
    "namespace": {
      "description": "namespace for the generated code",
      "replaces": "SnapflowCQRS",
      "type": "parameter"
    }
  }
}